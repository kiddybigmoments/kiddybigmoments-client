    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "typescript": "~2.5.3"
  }
